<div className="hamburger-nav">
    <input type="checkbox" name="toggle-menu" id="toggle-menu"></input>
    <label for="toggle-menu" className="menu-icon" id="hamburger-icon">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512">
            <path
                d="M0 96C0 78.3 14.3 64 32 64H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32C14.3 128 0 113.7 0 96zM0 256c0-17.7 14.3-32 32-32H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32c-17.7 0-32-14.3-32-32zM448 416c0 17.7-14.3 32-32 32H32c-17.7 0-32-14.3-32-32s14.3-32 32-32H416c17.7 0 32 14.3 32 32z" />
        </svg>
    </label>
    <nav className="nav" id="nav-header">
        <a href="/classes">Classes</a>
        <a href="/blogs">Blogs</a>
        <!-- <a href="/locations">Locations</a> -->
        <% if (accessRole) { %>
            <span>Hello <%= firstName %></span>
            <% } %>
                <% if (!accessRole) { %>
                    <a href="/login">Login</a>
                    <a href="/signup">Signup</a>
                    <% } else if (accessRole==="admin" ) { %>
                        <a href="/manage_users">Manage Users</a>
                        <a href="/manage_classes">Manage Classes</a>
                        <a href="/manage_bookings">Manage Bookings</a>
                        <a href="/manage_blogs">Manage Blogs</a>
                        <% } else if (accessRole==="trainer" ) { %>
                            <a href="/manage_classes">Manage Classes</a>
                            <% } else if (accessRole==="member" ) { %>
                                <a href="/my_bookings">My Bookings</a>
                                <a href="/my_blogs">My Blogs</a>
                                <a href="/my_profile">My Profile</a>
                                <% } %>
                                    <% if (accessRole) { %>
                                        <a href="/logout">Logout</a>
                                        <% } %>
    </nav>

</div>


